    <view-state id="login">
        <transition on="ok" to="dashboard"></transition>
    </view-state>

    <view-state id="dashboard">
        <transition on="alle" to="dashboard">
            <evaluate expression="tech42013.toggleWeniger()"></evaluate>
        </transition>
        <transition on="wenige" to="dashboard">
            <evaluate expression="tech42013.toggleWeniger()"></evaluate>
        </transition>
    </view-state>


</flow>